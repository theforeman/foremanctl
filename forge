#!/bin/bash

OBSAH_NAME=forge
OBSAH_DATA=development
OBSAH_INVENTORY=./inventories/
export OBSAH_NAME OBSAH_DATA OBSAH_INVENTORY

ANSIBLE_COLLECTIONS_PATH=./build/collections/forge
ANSIBLE_COLLECTIONS_SCAN_SYS_PATH=false
export ANSIBLE_COLLECTIONS_PATH ANSIBLE_COLLECTIONS_SCAN_SYS_PATH

exec obsah "$@"
